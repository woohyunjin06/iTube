<UserControl x:Class="iTube.Control.LoginControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:iTube.Control"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid Grid.Row="0">
            <!--0<Image Source="../Resources/kimbablogo.png" Height="200" DockPanel.Dock="Top" VerticalAlignment="Center"></Image>-->
        </Grid>
        <Grid Grid.Row="1">
            <ProgressBar x:Name="progressBar" Value="{Binding Values, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Stretch" MinHeight="30" VerticalAlignment="Center" Margin="20,0,20,0" Maximum="50" Minimum="0"/>
        </Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
    </Grid>
</UserControl>
